# Configuração do Bridge SIP-ConnectyCube com AMI

# === CONFIGURAÇÕES SIP ===
SIP_DOMAIN=localhost
SIP_PORT=5060
SIP_USERNAME=bridge_user
SIP_PASSWORD=bridge_password
SIP_REGISTRAR=sip:localhost:5060

# === CONFIGURAÇÕES AMI (Asterisk Manager Interface) ===
AMI_HOST=localhost
AMI_PORT=5038
AMI_USERNAME=bridge_ami
AMI_SECRET=bridge_secret_123

# === CONFIGURAÇÕES CONNECTYCUBE ===
CONNECTYCUBE_APP_ID=12345
CONNECTYCUBE_AUTH_KEY=sua_auth_key_aqui
CONNECTYCUBE_AUTH_SECRET=sua_auth_secret_aqui  
CONNECTYCUBE_ACCOUNT_KEY=sua_account_key_aqui

# === CONFIGURAÇÕES DO BRIDGE ===
# Modo de operação: sip-only | ami-only | hybrid
BRIDGE_MODE=hybrid

# Porta do servidor bridge
BRIDGE_PORT=3000

# === CONFIGURAÇÕES DE DESENVOLVIMENTO ===
NODE_ENV=development
LOG_LEVEL=debug

# === CONFIGURAÇÕES OPCIONAIS ===
# Timeout para chamadas (segundos)
CALL_TIMEOUT=300

# Intervalo de keep-alive AMI (milissegundos)
AMI_KEEPALIVE_DELAY=30000

# Range de portas RTP
RTP_PORT_MIN=10000
RTP_PORT_MAX=20000
